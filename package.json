{
  "name": "zod-message-validator",
  "license": "MIT",
  "version": "0.0.2",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "tsup src/index.ts --format cjs,esm --dts && node dist/index.js",
    "build": "tsup src/index.ts --format cjs,esm --dts",
