    "ci": "pnpm run lint && pnpm run build",
    "release": "pnpm run lint && pnpm run build && changeset publish",
    "test": "vitest --run"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "zod": "^3.23.8"
  }
}